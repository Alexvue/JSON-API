<Window x:Class="JSON_API.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_API"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tb_city" Margin="10,10,110,380" TextWrapping="Wrap"/>
        <Button x:Name="bt_know" Content="Узнать" Margin="687,10,10,380" Click="Bt_know_Click"/>
        <TextBlock x:Name="tb_information" Margin="10,55,10,10" TextWrapping="Wrap"/>
    </Grid>
</Window>
